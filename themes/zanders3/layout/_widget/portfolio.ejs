<div class="widget tag">
  <h3 class="title">Portfolio</h3>
  <ul class="entry">
  <%  var posts = site.categories.findOne({name:"Portfolio"}).posts;
	  posts.sort('date', 1).each(function(item){ %>
    <li><a href="<%- config.root %><%- item.path %>"><%= item.title %></a></li>
  <% }); %>
  </ul>
</div>